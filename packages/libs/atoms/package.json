{
  "name": "@marcosgotze/atoms",
  "version": "1.0.7-alpha.58",
  "main": "dist/atoms.umd.min.js",
  "files": [
    "dist/*"
  ],
  "scripts": {
    "build": "npm run build:umd & npm run build:es & npm run build:unpkg",
    "build:umd": "rollup --config build/rollup.config.js --format umd --file dist/atoms.umd.js",
    "build:es": "rollup --config build/rollup.config.js --format es --file dist/atoms.esm.js",
    "build:unpkg": "rollup --config build/rollup.config.js --format iife --file dist/atoms.min.js"
  },
  "devDependencies": {
    "@rollup/plugin-buble": "^0.21.3",
    "@rollup/plugin-commonjs": "^11.1.0",
    "rollup": "^1.17.0",
    "rollup-plugin-vue": "^5.0.1",
    "vue": "^2.6.10",
    "vue-template-compiler": "^2.6.10"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/marcosgotze/nina.git"
  },
  "bugs": {
    "url": "https://github.com/marcosgotze/nina/issues"
  },
  "homepage": "https://github.com/marcosgotze/nina#readme",
  "publishConfig": {
    "@marcosgotze:registry": "https://npm.pkg.github.com/"
  }
}
